%nav.navbar
  .navbar-left
    %div.logo Calendar

  .navbar-right
    - if current_user
      %span.user-email= current_user.email
      = button_to "Logout", logout_path, method: :delete, class: "btn"
    - else
      = link_to "Login", login_path, class: "btn"
      = link_to "Register", signup_path, class: "btn"
